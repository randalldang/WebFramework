<body>
	
</body>
</html>

	<div id="wrapper">
		<div id="wrappertop"></div>

		<div id="wrappermiddle">
			<div class="head">
					<i class="fa fa-send"></i>
					<label>马上快递运营中心</label>
			</div>

			<div id="username_input">

				<div id="username_inputleft"></div>

				<div id="username_inputmiddle">
					<input type="text" name="username" id="username" value="用户名">
					<img id="username_icon" src="./resources/images/mailicon.png" alt="">
				</div>

				<div id="username_inputright"></div>

			</div>
			
			<div id="password_input">

				<div id="password_inputleft"></div>

				<div id="password_inputmiddle">
					<input type="password" name="password" id="password" value="password">
					<img id="password_icon" src="./resources/images/passicon.png" alt="">
				</div>

				<div id="password_inputright"></div>

			</div>
			<div class="login_error error"></div>
			<div class="submit_div">
				<button type="button" id="submit" class="btn btn-success submit_btn">登录</button>
      </div>
		</div>

		<div id="wrapperbottom"></div>
		
	</div>
	<script src="resources/js/jquery-1.11.1.min.js"></script>
	<script src="resources/js/login.js"></script>
